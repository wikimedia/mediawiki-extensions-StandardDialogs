	"url": "https://en.wiki.bluespice.com/wiki/Reference:StandardDialogs",
	"descriptionmsg": "standarddialogs-desc",
	"requires": {
		"MediaWiki": ">= 1.35.0",
		"extensions": {
			"OOJSPlus": "*"
		}

